    <!-- App Intro -->
    <string name="app_intro_frag_1_title">Welcome to Five Prayers Android</string>
    <string name="app_intro_frag_1_description">An open source set of muslim tools, lightweight and ads free</string>

    <string name="app_intro_frag_2_title">Privacy Policy</string>
    <string name="app_intro_frag_2_description">Five Prayers Android project aims to provide a private,
        anonymous and ads free experience for all users. Therefore, the app does not collect any data without your consent</string>

    <string name="app_intro_frag_3_title">We need your permission</string>
    <string name="app_intro_frag_3_description">In order to calculate prayer timings effectively,
        this application need your permission to use your localisation capabilities. (recommended)</string>

    <string name="app_intro_frag_4_title">Enjoy !</string>
    <string name="app_intro_frag_4_description">Five Prayer Android introduces great features and capabilities for all users,
        like : Worldwide Prayer Timings, different calculation methods, notifications, reads Adhan, Qibla direction and more</string>

